<h1>My tasks</h1>
<ul>
  <% @tasks.each do |task| %>
    <input type="checkbox" <%= 'checked' if task.completed %>>
    <%= link_to task.title, task_path(task.id) %>
    <%= link_to "Delete", tasks_path(task.id), method: :delete %>
    <br>
  <% end %>	
</ul>

<%= link_to "Add new task!", new_path %>
